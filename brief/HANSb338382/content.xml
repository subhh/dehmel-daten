005"/>gutes von Dir &amp; <rs ref="http://data.dehmel.digital/register.xml#gnd_1025140060" type="Person">Ernst</rs>. Es umarmt dich Dein <rs ref="http://data.dehmel.digital/register.xml#gnd_1067218343" type="Person">Julius</rs>. <lb xml:id="HANSb338382-line-00000006"/> <lb xml:id="HANSb338382-line-00000007"/> <lb xml:id="HANSb338382-line-00000008"/> <lb xml:id="HANSb338382-line-00000009"/> <lb xml:id="HANSb338382-line-00000010"/> <lb xml:id="HANSb338382-line-00000011"/> <lb xml:id="HANSb338382-line-00000012"/> <lb xml:id="HANSb338382-line-00000013"/> <lb xml:id="HANSb338382-line-00000014"/> <lb xml:id="HANSb338382-line-00000015"/> <lb xml:id="HANSb338382-line-00000016"/> <lb xml:id="HANSb338382-line-00000017"/> <lb xml:id="HANSb338382-line-00000018"/> <lb xml:id="HANSb338382-line-00000019"/> <lb xml:id="HANSb338382-line-00000020"/><pb xml:id="HANSb338382-page-00000002" facs="https://iiif.sub.uni-hamburg.de/object/HANSb15381/canvas/PHYS_0002"/> <lb xml:id="HANSb338382-line-00000021"/> <lb xml:id="HANSb338382-line-00000022"/> <lb xml:id="HANSb338382-line-00000023"/> <lb xml:id="HANSb338382-line-00000024"/> <lb xml:id="HANSb338382-line-00000025"/> <lb xml:id="HANSb338382-line-00000026"/> <lb xml:id="HANSb338382-line-00000027"/> <lb xml:id="HANSb338382-line-00000028"/> <lb xml:id="HANSb338382-line-00000029"/> <lb xml:id="HANSb338382-line-00000030"/> <lb xml:id="HANSb338382-line-00000031"/> <lb xml:id="HANSb338382-line-00000032"/> <lb xml:id="HANSb338382-line-00000033"/> <lb xml:id="HANSb338382-line-00000034"/> <lb xml:id="HANSb338382-line-00000035"/> <lb xml:id="HANSb338382-line-00000036"/> <lb xml:id="HANSb338382-line-00000037"/> <lb xml:id="HANSb338382-line-00000038"/> <lb xml:id="HANSb338382-line-00000039"/><pb xml:id="HANSb338382-page-00000003" facs="https://iiif.sub.uni-hamburg.de/object/HANSb338514/canvas/PHYS_0003"/> <lb xml:id="HANSb338382-line-00000040"/> <lb xml:id="HANSb338382-line-00000041"/> <lb xml:id="HANSb338382-line-00000042"/> <lb xml:id="HANSb338382-l